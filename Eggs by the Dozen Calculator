class MakeChange
{
   static void Main()
   {
     const int DOZEN = 12;
     int chicken1 = 30, chicken2 = 40, chicken3 = 35, chicken4 = 22;
     int total;
     int dozens, leftover;
     total = chicken1 + chicken2 + chicken3 + chicken4;
     dozens = total / DOZEN;
     leftover = total % DOZEN;
     WriteLine("{0} eggs is {1} dozen and {2} eggs", total, dozens, leftover); // spelling mistake in leftOver should be corrected as leftover
   }
}
