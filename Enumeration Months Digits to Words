
    {
      January = 1,
      February = 2,
      March = 3,
      April = 4,
      May = 5,
      June =6,
      July = 7,
      August = 8,
      September = 9,
      October = 10,
      November = 11,
      December = 12,

    }
   static void Main()
   {
     int monthIndex = 0;
     Month MonthName;
     WriteLine("Enter the Month #: ");
     monthIndex = int.Parse(ReadLine());
     MonthName = (Month)monthIndex;
     WriteLine(MonthName);
     
   }
