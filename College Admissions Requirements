   static void Main()
   {
     double gpa;
     int testScore;
     const double MINGPA = 3.0;
     const int ADMIN1 = 60, ADMIN2 = 00;
     string inputString;
     WriteLine("Enter your Grade Point Average: ");
     inputString = ReadLine();
     gpa = Convert.ToDouble(inputString);
     WriteLine("Enter your Test Score: ");
     inputString = ReadLine();
     testScore = Convert.ToInt32(inputString);
     if (gpa >= MINGPA)
         if (testScore >= ADMIN1)
         WriteLine("Accept");
         else WriteLine("Reject");
         if (testScore >= ADMIN2)
         WriteLine("Accept");

         else 
            WriteLine("Reject");
            
   }
}
