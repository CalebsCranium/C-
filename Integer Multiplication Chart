using System;

class MultiplicationTable
{
    static void Main()
    {
        Console.Write("Enter an integer: ");
        int num = int.Parse(Console.ReadLine());

        Console.WriteLine("Multiplication table for {0}:", num);
        for (int i = 1; i <= 10; i++)
        {
            int result = num * i;
            Console.WriteLine("{0} x {1} = {2}", i, num, result);
        }

        Console.ReadLine(); // to keep console window open
    }
}
